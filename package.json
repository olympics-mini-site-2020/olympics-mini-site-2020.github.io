{
  "private": true,
  "scripts": {
    "copy-vendor-js": "./scripts/copy-vendor-js.sh",
    "minify:js": "uglifyjs src/javascripts/vendor/*.js -o src/javascripts/vendor.min.js",
    "clean": "rm -rf src/javascripts/vendor.min.js",
    "start": "http-server",
    "build:js": "npm run clean && npm run copy-vendor-js && npm run minify:js",
    "dev": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --colors"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "core-js": "^3.6.4",
    "d3": "^5.15.0",
    "d3-selection-multi": "^1.0.1"
  }
}
